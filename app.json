{
  "expo": {
    "name": "MiCultura Planner",
    "slug": "micultura-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios/AppIcon~ios-marketing.png",
    "scheme": "micultura-planner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "gov.micultura.planner",
      "infoPlist": {
        "NSCameraUsageDescription": "Esta aplicación necesita acceso a la cámara para el reconocimiento facial en eventos del Ministerio de Cultura."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android/play_store_512.png",
        "backgroundColor": "#1B4D8C"
      },
      "package": "gov.micultura.planner",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a820d240-9fa1-44d3-bfc8-5d0476fa8828"
      }
    },
    "owner": "ghxstloner"
  }
}